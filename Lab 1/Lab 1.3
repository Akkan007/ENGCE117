#include <stdio.h>
#include <stdlib.h>

int *GetSet(int *n) {
    int i;
    printf("Enter number of elements : ");
    scanf("%d", n);

    int *arr = (int *)malloc((*n) * sizeof(int));
    if (arr == NULL) {
        printf("Memory allocation failed\n");
        exit(1);
    }

    printf("Enter %d elements:\n", *n);
    for (i = 0; i < *n; i++) {
        scanf("%d", &arr[i]);
    }

    return arr;
}

int main() {
    int *data, num;

    data = GetSet(&num);

    printf("\nYou entered:\n");
    for (int i = 0; i < num; i++) {
        printf("%d ", data[i]);
    }

    free(data);
    return 0;
}
