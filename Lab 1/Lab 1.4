#include <stdio.h>
#include <stdlib.h>

int GetSet( int data[] ) {
    int n, i;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &data[i]);
    }

    return n;
}

int main() {
    int data[100];
    int num;

    num = GetSet(data);

    printf("\nNumber of elements = %d\n", num);
    printf("Set = ");
    for (int i = 0; i < num; i++) {
        printf("%d ", data[i]);
    }

    return 0;
}
